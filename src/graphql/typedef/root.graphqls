type Query {
    virksomheter: [Virksomhet!]!
    mineSykmeldte: [PreviewSykmeldt!]
    sykmelding(sykmeldingId: ID!): Sykmelding
    soknad(soknadId: ID!): Soknad
}

type Mutation {
    read(type: ReadType!, id: ID!): Boolean
    unlinkSykmeldt(sykmeldtId: ID!): Boolean
}

enum ReadType {
    Soknad
    Sykmelding
    Hendelse
}

type Virksomhet {
    orgnummer: String!
    navn: String!
}
