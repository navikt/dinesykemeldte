type Query {
    viewer: Viewer!
    sykmeldinger(personUuid: String!): [Sykmelding!]!
    dineSykmeldte(virksomhet: String!): [Person!]!
}

type Viewer {
    personNummer: String!
    virksomheter: [Virksomhet!]!
}

type Virksomhet {
    uuid: String!
    navn: String!
}

type Person {
    uuid: String!
    fodselsNummer: String!
    navn: String!
}

type Sykmelding {
    dato: String!
}
