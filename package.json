        "@graphql-tools/merge": "^8.3.12",
        "@graphql-tools/schema": "^9.0.10",
        "@graphql-typed-document-node/core": "^3.1.1",
        "@mdx-js/mdx": "^2.1.5",
        "@navikt/dinesykmeldte-sidemeny": "^2.0.0",
        "@navikt/ds-css": "^2.0.5",
        "@navikt/ds-icons": "^2.0.5",
        "@navikt/ds-react": "^2.0.5",
        "@navikt/fnrvalidator": "^1.3.3",
        "@navikt/nav-dekoratoren-moduler": "^1.8.1",
        "@navikt/next-auth-wonderwall": "^1.0.0",
        "@navikt/next-logger": "^1.13.0",
        "@next/bundle-analyzer": "^13.0.0",
        "@reduxjs/toolkit": "^1.9.0",
        "apollo-server-micro": "^3.11.1",
        "classnames": "^2.3.2",
        "date-fns": "^2.29.3",
        "fuse.js": "^6.6.2",
        "graphql": "16.6.0",
        "graphql-tag": "^2.12.6",
        "html-react-parser": "^1.4.14",
        "jsdom": "^20.0.3",
        "lodash.throttle": "^4.1.1",
        "micro": "^9.4.1",
        "next": "12.3.4",
        "next-logger": "3.0.1",
        "next-mdx-remote": "^4.2.0",
        "pino": "^8.7.0",
        "pino-pretty": "^9.1.1",
        "prom-client": "^14.1.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "remeda": "^1.3.0",
