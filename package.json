{
    "name": "dinesykmeldte",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@3.2.0",
    "engines": {
        "node": "16"
    },
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "NODE_OPTIONS='--inspect' next dev | pino-pretty",
        "build": "next build",
        "test:watch": "jest --watch",
        "test": "jest",
        "lint": "next lint",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "start:prod": "NODE_OPTIONS='-r next-logger' next start",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "dependencies": {
        "@apollo/client": "^3.6.9",
        "@graphql-tools/graphql-file-loader": "^7.5.3",
        "@graphql-tools/load": "^7.7.5",
        "@graphql-tools/merge": "^8.3.4",
        "@graphql-tools/schema": "^9.0.2",
        "@graphql-typed-document-node/core": "^3.1.1",
        "@mdx-js/mdx": "^2.1.3",
        "@navikt/dinesykmeldte-sidemeny": "^1.3.0",
        "@navikt/ds-css": "^1.2.4",
        "@navikt/ds-icons": "^1.2.4",
        "@navikt/ds-react": "^1.2.4",
        "@navikt/fnrvalidator": "^1.3.0",
        "@navikt/nav-dekoratoren-moduler": "^1.6.18",
        "@navikt/next-auth-wonderwall": "^0.8.0",
        "@navikt/next-logger": "^1.11.0",
        "@next/bundle-analyzer": "^12.2.5",
        "@reduxjs/toolkit": "^1.8.5",
        "@sentry/nextjs": "^7.12.0",
        "@testing-library/react-hooks": "^8.0.1",
        "apollo-server-micro": "^3.10.2",
        "classnames": "^2.3.1",
        "date-fns": "^2.29.2",
        "fuse.js": "^6.6.2",
        "graphql": "16.6.0",
        "graphql-tag": "^2.12.6",
        "html-react-parser": "^1.4.14",
        "jsdom": "^20.0.0",
        "lodash.throttle": "^4.1.1",
        "micro": "^9.4.1",
        "next": "12.2.5",
        "next-logger": "3.0.1",
        "next-mdx-remote": "^4.1.0",
        "pino": "^8.5.0",
        "pino-pretty": "^9.1.0",
        "prom-client": "^14.1.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.2",
        "remeda": "^1.1.0",
        "ua-parser-js": "^1.0.2",
        "zod": "3.18.0"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^3.2.1",
        "@graphql-codegen/cli": "^2.12.0",
        "@graphql-codegen/fragment-matcher": "^3.3.1",
        "@graphql-codegen/typed-document-node": "^2.3.3",
        "@graphql-codegen/typescript-operations": "^2.5.3",
        "@graphql-codegen/typescript-resolvers": "^2.7.3",
        "@navikt/eslint-config-teamsykmelding": "^0.21.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/react": "18.0.18",
        "@types/ua-parser-js": "^0.7.36",
        "concurrently": "^7.3.0",
