{
    "name": "dinesykmeldte",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@3.2.0",
    "engines": {
        "node": "16"
    },
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "NODE_OPTIONS='--inspect' next dev | pino-pretty",
        "build": "next build",
        "test:watch": "jest --watch",
        "test": "jest",
        "lint": "next lint",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "start:prod": "NODE_OPTIONS='-r next-logger' next start",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "dependencies": {
        "@amplitude/analytics-browser": "^1.6.3",
        "@apollo/client": "^3.7.1",
        "@graphql-tools/graphql-file-loader": "^7.5.11",
