{
    "name": "dinesykmeldte",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@3.0.2",
    "engines": {
        "node": "16"
    },
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "NODE_OPTIONS='--inspect' next dev",
        "build": "next build",
        "lint": "next lint",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "start:prod": "NODE_OPTIONS='-r next-logger/presets/next-only' next start",
        "postinstall": "husky install"
    },
    "dependencies": {
        "@navikt/ds-css": "^0.10.0",
        "@navikt/ds-icons": "^0.5.7",
        "@navikt/ds-react": "^0.11.6",
        "@navikt/fnrvalidator": "^1.1.4",
        "@navikt/nav-dekoratoren-moduler": "^1.6.4",
        "@sentry/nextjs": "^6.13.3",
        "apollo-server-micro": "^2.25.2",
        "classnames": "^2.3.1",
        "graphql": "^15.6.1",
        "html-react-parser": "^1.4.0",
        "jsdom": "^16.7.0",
        "lodash.throttle": "^4.1.1",
        "next": "^11.1.2",
        "next-compose-plugins": "^2.2.1",
        "next-logger": "^2.2.0",
        "node-cache": "^5.1.2",
        "node-fetch": "^2.6.5",
        "pino": "^6.13.3",
        "pino-pretty": "^6.0.0",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-query": "^3.28.0"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^3.1.0",
        "@graphql-codegen/cli": "^2.2.1",
        "@graphql-codegen/typescript-operations": "^2.1.8",
        "@graphql-codegen/typescript-react-query": "^2.1.3",
        "@graphql-codegen/typescript-resolvers": "^2.3.2",
        "@graphql-tools/load-files": "^6.5.1",
        "@graphql-tools/merge": "^8.2.0",
        "@graphql-tools/schema": "^8.3.0",
        "@next/bundle-analyzer": "^11.1.2",
        "@types/pino": "^6.3.12",
        "@types/react": "17.0.32",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^5.2.0",
        "concurrently": "^6.3.0",
        "eslint": "^7.32.0",
        "eslint-config-next": "^11.1.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "graphql-tag": "^2.12.5",
        "husky": "^7.0.4",
        "lint-staged": "11.1.2",
        "prettier": "^2.4.1",
        "typescript": "4.4.4"
    }
}
