{
    "name": "dinesykmeldte",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@3.2.0",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "dev": "yarn start",
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "NODE_OPTIONS='--inspect' next dev | pino-pretty",
        "build": "next build",
        "test:watch": "jest --watch",
        "test": "jest",
        "lint": "next lint",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "start:prod": "NODE_OPTIONS='-r next-logger' next start",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "dependencies": {
        "@apollo/client": "^3.7.14",
        "@apollo/server": "^4.7.1",
        "@as-integrations/next": "^1.3.0",
        "@grafana/faro-web-sdk": "^1.0.5",
        "@graphql-tools/graphql-file-loader": "^7.5.17",
        "@graphql-tools/load": "^7.8.14",
        "@graphql-tools/merge": "^8.4.1",
        "@graphql-tools/schema": "^9.0.19",
        "@graphql-typed-document-node/core": "^3.2.0",
        "@mdx-js/mdx": "^2.3.0",
        "@navikt/aksel-icons": "^2.9.7",
        "@navikt/dinesykmeldte-sidemeny": "^3.1.0",
        "@navikt/ds-css": "^2.9.7",
        "@navikt/ds-icons": "^2.9.7",
        "@navikt/ds-react": "^2.9.7",
        "@navikt/fnrvalidator": "^1.3.3",
        "@navikt/nav-dekoratoren-moduler": "^2.0.1",
        "@navikt/next-auth-wonderwall": "^1.3.0",
        "@navikt/next-logger": "^1.19.0",
        "@next/bundle-analyzer": "^13.4.0",
        "@reduxjs/toolkit": "^1.9.5",
        "autoprefixer": "^10.4.14",
        "classnames": "^2.3.2",
        "date-fns": "^2.30.0",
        "fuse.js": "^6.6.2",
        "graphql": "16.6.0",
        "graphql-tag": "^2.12.6",
        "jsdom": "^22.0.0",
        "lodash.throttle": "^4.1.1",
        "next": "13.3.4",
        "next-logger": "3.0.2",
        "next-mdx-remote": "^4.4.1",
        "nextleton": "^0.5.0",
        "pino": "^8.12.1",
        "pino-pretty": "^10.0.0",
        "prom-client": "^14.2.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "remeda": "^1.14.0",
        "tailwind-merge": "^1.12.0",
        "ua-parser-js": "^1.0.35",
        "zod": "3.21.4"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^4.0.1",
        "@graphql-codegen/cli": "3.3.1",
        "@graphql-codegen/fragment-matcher": "^4.0.1",
        "@graphql-codegen/typed-document-node": "^4.0.1",
        "@graphql-codegen/typescript-operations": "^3.0.4",
        "@graphql-codegen/typescript-resolvers": "^3.2.1",
        "@navikt/ds-tailwind": "^2.8.16",
        "@navikt/eslint-config-teamsykmelding": "^1.6.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^14.4.3",
        "@types/react": "18.2.5",
        "@types/ua-parser-js": "^0.7.36",
        "concurrently": "^8.0.1",
        "dotenv": "^16.0.3",
        "eslint": "^8.39.0",
        "eslint-config-next": "^13.4.0",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "lint-staged": "13.2.2",
        "next-router-mock": "^0.9.3",
        "node-gyp": "^9.3.1",
        "postcss": "^8.4.23",
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.2.7",
        "react-test-renderer": "^18.2.0",
        "tailwindcss": "^3.3.1",
        "typescript": "5.0.4"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "postcss-modules/no-undef-class": "off",
            "postcss-modules/no-unused-class": "off"
        }
    }
}
