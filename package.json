{
    "name": "dinesykmeldte",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "16",
        "npm": "7"
    },
    "scripts": {
        "start": "concurrently -n codegen,next \"npm run start:watch\" \"npm run gen:watch\"",
        "start:watch": "next dev",
        "build": "next build",
        "lint": "next lint",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "start:prod": "NODE_OPTIONS='-r next-logger/presets/next-only' next start",
        "postinstall": "./scripts/strip-less"
    },
    "dependencies": {
        "@graphql-tools/load-files": "^6.3.2",
